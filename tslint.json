{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"rules": {
		"max-line-length": [true, 90],
		"object-literal-sort-keys": false,
		"no-console": false,
		"indent": [true, "tabs", 4],
		"no-trailing-whitespace": false,
		"no-unused-expression": false,
		"no-var-requires": false,
		"max-classes-per-file": false
	},
	"rulesDirectory": []
}
